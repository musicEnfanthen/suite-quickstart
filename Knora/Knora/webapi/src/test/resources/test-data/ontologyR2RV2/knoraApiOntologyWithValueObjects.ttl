      owl:cardinality 1;
      owl:onProperty knora-api:valueCreationDate
    ], [ a owl:Restriction;
      knora-api:isInherited true;
      owl:maxCardinality 1;
      owl:onProperty knora-api:valueHasComment
    ] .

