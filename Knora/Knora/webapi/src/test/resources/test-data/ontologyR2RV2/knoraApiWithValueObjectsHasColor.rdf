<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:knora-api="http://api.knora.org/ontology/knora-api/v2#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:salsah-gui="http://api.knora.org/ontology/salsah-gui/v2#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
<owl:Ontology rdf:about="http://api.knora.org/ontology/knora-api/v2">
	<knora-api:attachedToProject rdf:resource="http://www.knora.org/ontology/knora-base#SystemProject"/>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The knora-api ontology in the complex schema</rdfs:label>
</owl:Ontology>
<owl:ObjectProperty rdf:about="http://api.knora.org/ontology/knora-api/v2#hasColor">
	<knora-api:isEditable rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</knora-api:isEditable>
	<knora-api:isResourceProperty rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</knora-api:isResourceProperty>
	<knora-api:objectType rdf:resource="http://api.knora.org/ontology/knora-api/v2#ColorValue"/>
	<knora-api:subjectType rdf:resource="http://api.knora.org/ontology/knora-api/v2#Region"/>
	<salsah-gui:guiAttribute rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ncolors=8</salsah-gui:guiAttribute>
	<salsah-gui:guiElement rdf:resource="http://api.knora.org/ontology/salsah-gui/v2#Colorpicker"/>
	<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the color of a region.</rdfs:comment>
	<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Color</rdfs:label>
	<rdfs:subPropertyOf rdf:resource="http://api.knora.org/ontology/knora-api/v2#hasValue"/>
</owl:ObjectProperty>

</rdf:RDF>