@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix physical: <http://www.knora.org/ontology/shared/physical-resource#>.
@prefix event: <http://www.knora.org/ontology/shared/event#>.
@prefix human: <http://www.knora.org/ontology/shared/human#>.
@prefix concept: <http://www.knora.org/ontology/shared/concept#>.
@prefix language: <http://www.knora.org/ontology/shared/language#>.
@prefix document: <http://www.knora.org/ontology/shared/document#>.
@prefix text: <http://www.knora.org/ontology/shared/text#>.
@prefix text-structure: <http://www.knora.org/ontology/shared/text-structure#>.
@prefix textedit: <http://www.knora.org/ontology/shared/text-editing#>.
@prefix infocar: <http://www.knora.org/ontology/shared/information-carrier#>.
@prefix image: <http://www.knora.org/ontology/shared/image#>.
@prefix litera: <http://www.knora.org/ontology/shared/literature#>.
@prefix publish: <http://www.knora.org/ontology/shared/publishing#>.
@prefix knora-base: <http://www.knora.org/ontology/knora-base#>.

@base <http://www.knora.org/ontology/shared/kuno-raeber>.
@prefix kuno-raeber: <http://www.knora.org/ontology/shared/kuno-raeber#>.

<http://www.knora.org/ontology/shared/kuno-raeber>
	a owl:Ontology;
	dct:title "An ontology about the lyric of Kuno Raeber"@en;
	dct:description """Formal description of elements in the online publication of the lyric work of the Swiss poet Kuno Raeber."""@en;
	dct:creator "Hans Cools, MD, knowledge engineer, ontologist, software developer, NIE-INE, Basel University, Switzerland"@en;

	knora-base:attachedToProject <http://rdfh.ch/projects/004D>.

#
# CLASSES
#

kuno-raeber:Work
	a owl:Class;
	rdfs:label "Kuno Raeber work"@en, "Kuno Raeber-Werk"@de;
	rdfs:comment """Work by Kuno Raeber."""@en, """Werk von Kuno Raeber."""@de;
	rdfs:subClassOf concept:IndividualWork.

kuno-raeber:Text
	a owl:Class;
	rdfs:label "Kuno Raeber text expression"@en, "Kuno Raeber-Text-Expression"@de;
	rdfs:comment """Text expression authored by Kuno Raeber."""@en;
	rdfs:subClassOf text:IndividualExpression, [
		a owl:Restriction; owl:onProperty text:hasAlias; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasSpecialDescription; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:isFinalVersion; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:isInSwissGerman; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:isPartOfCycle; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty knora-base:seqnum; owl:minCardinality "0"^^xsd:nonNegativeInteger].

kuno-raeber:Poem
	a owl:Class;
	rdfs:label "Kuno Raeber poem"@en, "Kuno Raeber-Gedicht"@de;
	rdfs:comment """Poem authored by Kuno Raeber."""@en, """Gedicht von Kuno Raeber."""@de;
	rdfs:subClassOf litera:Poem, kuno-raeber:Text, [
		a owl:Restriction; owl:onProperty kuno-raeber:poemHasTitle; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasDetailDescription; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
#		a owl:Restriction; owl:onProperty text:hasSpecialDescription; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:hasSameEditionAsIn; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:hasSameEditionAsInValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:hasReferencePoem; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:hasReferencePoemValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:hasReferenceTitle; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:hasStrophe; owl:cardinality "1"^^xsd:nonNegativeInteger].

kuno-raeber:ScientificEdition
	a owl:Class;
	rdfs:label "Kuno Raeber scientific edition"@en, "Kuno Raeber wissenschaftliche Edition"@de;
	rdfs:comment """Scientific edition of a text authored by Kuno Raeber."""@en, """Wissenschaftliche Edition eines von Kuno Raeber verfassten Texts."""@de;
	rdfs:subClassOf textedit:ScientificEdition, [
		a owl:Restriction; owl:onProperty language:hasContent; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:hasPageIndicator; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:hasPageNumberIndicator; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:hasColumnIndicator; owl:minCardinality "0"^^xsd:nonNegativeInteger].

kuno-raeber:DiplomaticTranscription
	a owl:Class;
	rdfs:label "diplomatic transcription"@en, "diplomatische Umschrift"@de;
	rdfs:comment """Diplomatic transcription of a text authored by Kuno Raeber."""@en, """Diplomatische Umschrift eines von Kuno Raeber verfassten Texts."""@de;
	rdfs:subClassOf textedit:DiplomaticTranscription, [
		a owl:Restriction; owl:onProperty language:hasContent; owl:minCardinality "0"^^xsd:nonNegativeInteger], [ # needed due to restriction in dhl-knora
		a owl:Restriction; owl:onProperty knora-base:seqnum; owl:minCardinality "0"^^xsd:nonNegativeInteger].

kuno-raeber:HandwrittenPoem
	a owl:Class;
	rdfs:label "handwritten poem"@en, "handgeschriebenes Gedicht"@de;
	rdfs:comment """Handwritten poem authored by Kuno Raeber."""@en, """Handgeschriebenes Gedicht von Kuno Raeber."""@de;
	rdfs:subClassOf kuno-raeber:Poem, text:HandwrittenText, [
		a owl:Restriction; owl:onProperty kuno-raeber:isOnPage; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:isOnPageValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

kuno-raeber:PoemNote
	a owl:Class;
	rdfs:label "poem note"@en, "Gedichtnotiz"@de;
	rdfs:comment """Poem note authored by Kuno Raeber."""@en, """Gedichtnotiz von Kuno Raeber."""@de;
	rdfs:subClassOf kuno-raeber:HandwrittenPoem, human:FirstVersion, [
		a owl:Restriction; owl:onProperty kuno-raeber:isInNotebook; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:isInNotebookValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

kuno-raeber:PostcardPoem
	a owl:Class;
	rdfs:label "postcard poem"@en, "Postkarten-Gedicht"@de;
	rdfs:comment """Postcard poem authored by Kuno Raeber."""@en, """Postkarten-Gedicht von Kuno Raeber."""@de;
	rdfs:subClassOf kuno-raeber:HandwrittenPoem, [
		a owl:Restriction; owl:onProperty kuno-raeber:isOnPostcard; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:isOnPostcardValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasNachlassPublicationDescription; owl:minCardinality "0"^^xsd:nonNegativeInteger].

kuno-raeber:TypewrittenPoem
	a owl:Class;
	rdfs:label "typewritten poem"@en, "maschinengeschriebenes Gedicht"@de;
	rdfs:comment """Typewritten poem authored by Kuno Raeber."""@en, """Maschinengeschriebenes Gedicht von Kuno Raeber."""@de;
	rdfs:subClassOf kuno-raeber:Poem, text:TypewrittenText.

kuno-raeber:PublicationPoem
	a owl:Class;
	rdfs:label "publication poem"@en, "Publikation-Gedicht"@de;
	rdfs:comment """Poem authored by Kuno Raeber, as published."""@en, """Gedicht von Kuno Raeber wie publiziert."""@de;
	rdfs:subClassOf kuno-raeber:Poem, publish:Expression, [
		a owl:Restriction; owl:onProperty kuno-raeber:isInPolyAuthorPublicationConvolute; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:isInPolyAuthorPublicationConvoluteValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

kuno-raeber:DiaryEntry
	a owl:Class;
	rdfs:label "diary entry"@en, "Tagebucheintrag"@de;
	rdfs:comment """Diary entry authored by Kuno Raeber."""@en;
	rdfs:subClassOf text:DiaryEntry, kuno-raeber:Text, [
		a owl:Restriction; owl:onProperty kuno-raeber:isInDiary; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:isInDiaryValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:isInDiaryConvolute; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:isInDiaryConvoluteValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

kuno-raeber:LetterExpression
	a owl:Class;
	rdfs:label "letter expression"@en, "Brief-Expression"@de;
	rdfs:comment """Letter expression written by Kuno Raeber."""@en;
	rdfs:subClassOf text:LetterExpression, text:WrittenText, kuno-raeber:Text, [
		a owl:Restriction; owl:onProperty text:hasSpecialDescription; owl:minCardinality "0"^^xsd:nonNegativeInteger].

kuno-raeber:Page
	a owl:Class;
	rdfs:label "page"@en, "Seite"@de;
	rdfs:comment """Page with a handwritten poem by Kuno Raeber."""@en, """Seite mit einem handgeschriebenen Gedicht Kuno Raebers."""@de;
	rdfs:subClassOf infocar:Page, knora-base:StillImageRepresentation, [
		a owl:Restriction; owl:onProperty knora-base:hasStillImageFileValue; owl:minCardinality "1"^^xsd:nonNegativeInteger].

kuno-raeber:Convolute
	a owl:Class;
	rdfs:label "convolute"@en, "Konvolut"@de;
	rdfs:comment """Convolute of which the elements are authored by Kuno Raeber."""@en;
	rdfs:subClassOf infocar:Convolute, [
		a owl:Restriction; owl:onProperty text:hasComment; owl:minCardinality "0"^^xsd:nonNegativeInteger].

kuno-raeber:PoemNotebook
	a owl:Class;
	rdfs:label "poem notebook"@en, "Gedichten-Notizbuch"@de;
	rdfs:comment """Notebook with poem drafts authored by Kuno Raeber."""@en;
	rdfs:subClassOf infocar:Manuscript, infocar:Notebook, kuno-raeber:Convolute, infocar:ArchiveElement, knora-base:StillImageRepresentation, [
		a owl:Restriction; owl:onProperty knora-base:hasStillImageFileValue; owl:minCardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:containsEarlierStagesOfManuscriptConvolute; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:containsEarlierStagesOfManuscriptConvoluteValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:containsEarlierStagesOfTypescriptConvolute; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:containsEarlierStagesOfTypescriptConvoluteValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

kuno-raeber:PoemManuscript
	a owl:Class;
	rdfs:label "poem manuscript"@en, "Gedicht-Manuskript"@de;
	rdfs:comment """Manuscript with a poem authored by Kuno Raeber."""@en;
	rdfs:subClassOf infocar:Leaf, infocar:Manuscript, infocar:ConvoluteElement, infocar:ArchiveElement, [
		a owl:Restriction; owl:onProperty kuno-raeber:isPartOfManuscriptConvolute; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:isPartOfManuscriptConvoluteValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

kuno-raeber:PoemManuscriptConvolute
	a owl:Class;
	rdfs:label "poem manuscript convolute"@en, "Gedicht-Manuskriptenkonvolut"@de;
	rdfs:comment """Poem manuscript convolute with poems authored by Kuno Raeber."""@en;
	rdfs:subClassOf kuno-raeber:Convolute, infocar:ArchiveElement, [
		a owl:Restriction; owl:onProperty kuno-raeber:containsLaterStagesOfNotebook; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:containsLaterStagesOfNotebookValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [	
		a owl:Restriction; owl:onProperty kuno-raeber:containsEarlierStagesOfTypescriptConvolute; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:containsEarlierStagesOfTypescriptConvoluteValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

kuno-raeber:PoemPostcard
	a owl:Class;
	rdfs:label "poem postcard"@en, "Gedicht-Postkarte"@de;
	rdfs:comment """Postcard with a poem authored by Kuno Raeber."""@en;
	rdfs:subClassOf infocar:Manuscript, infocar:Postcard, infocar:ArchiveElement, infocar:ConvoluteElement, knora-base:StillImageRepresentation, [
		a owl:Restriction; owl:onProperty kuno-raeber:isPartOfPostcardConvolute; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:isPartOfPostcardConvoluteValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty knora-base:hasStillImageFileValue; owl:minCardinality "1"^^xsd:nonNegativeInteger].

kuno-raeber:PoemPostcardConvolute
	a owl:Class;
	rdfs:label "poem postcard convolute"@en, "Gedicht-Postkartenkonvolut"@de;
	rdfs:comment """Poem postcard convolute with poems authored by Kuno Raeber."""@en;
	rdfs:subClassOf kuno-raeber:Convolute, knora-base:StillImageRepresentation, [
		a owl:Restriction; owl:onProperty knora-base:hasStillImageFileValue; owl:minCardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:containsLaterStagesOfNotebook; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:containsLaterStagesOfNotebookValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

kuno-raeber:PoemTypescript
	a owl:Class;
	rdfs:label "poem typescript"@en, "Gedicht-Typoskript"@de;
	rdfs:comment """Poem typescript by Kuno Raeber."""@en;
	rdfs:subClassOf infocar:Leaf, infocar:Typescript, infocar:ConvoluteElement, infocar:ArchiveElement, [
		a owl:Restriction; owl:onProperty kuno-raeber:isPartOfTypescriptConvolute; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:isPartOfTypescriptConvoluteValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

kuno-raeber:PoemTypescriptConvolute
	a owl:Class;
	rdfs:label "poem typescript convolute"@en, "Gedicht-Typoskriptenonvolut"@de;
	rdfs:comment """Poem typescript convolute with poems authored by Kuno Raeber."""@en;
	rdfs:subClassOf kuno-raeber:Convolute, infocar:ArchiveElement, [
		a owl:Restriction; owl:onProperty kuno-raeber:containsLaterStagesOfNotebook; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:containsLaterStagesOfNotebookValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:containsLaterStagesOfManuscriptConvolute; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:containsLaterStagesOfManuscriptConvoluteValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

kuno-raeber:PoemTypescriptConvoluteWithImage # to be removed when knora-base:hasRepresentation will be implemented in Knora
	a owl:Class;
	rdfs:label "poem typescript convolute with image"@en, "Gedicht-Typoskriptenonvolut mit Bild"@de;
	rdfs:comment """Poem typescript convolute with poems authored by Kuno Raeber, mit bild."""@en;
	rdfs:subClassOf kuno-raeber:PoemTypescriptConvolute, knora-base:StillImageRepresentation, [
		a owl:Restriction; owl:onProperty knora-base:hasStillImageFileValue; owl:minCardinality "1"^^xsd:nonNegativeInteger].

kuno-raeber:PrintedPoemBookPublication
	a owl:Class;
	rdfs:label "printed poem book publication"@en, "gedruckte Gedichtenbuchausgabe"@de;
	rdfs:comment """Printed publication of a book  with poems authored by Kuno Raeber."""@en;
	rdfs:subClassOf infocar:Book, publish:PrintedPublication, kuno-raeber:Convolute, knora-base:StillImageRepresentation, [
		a owl:Restriction; owl:onProperty knora-base:hasStillImageFileValue; owl:minCardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:containsLaterStagesOfNotebook; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:containsLaterStagesOfNotebookValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:containsLaterStagesOfManuscriptConvolute; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:containsLaterStagesOfManuscriptConvoluteValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:containsLaterStagesOfTypescriptConvolute; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:containsLaterStagesOfTypescriptConvoluteValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

kuno-raeber:PolyAuthorPublication
	a owl:Class;
	rdfs:label "poly-author publication"@en, "Mehrautorenpublikationen"@de;
	rdfs:comment """Publication of a poem authored by Kuno Raeber among expressions by other authors."""@en;
	rdfs:subClassOf publish:PolyAuthorPublication, [
		a owl:Restriction; owl:onProperty kuno-raeber:hasContentPartInPolyAuthorPublicationConvolute; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:hasContentPartInPolyAuthorPublicationConvoluteValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

kuno-raeber:PolyAuthorPublicationConvolute
	a owl:Class;
	rdfs:label "poly-author publication convolute"@en, "Mehrautorenpublikationen-Konvolut"@de;
	rdfs:comment """Convolute of poems authored by Kuno Raeber as published among expressions by other authors."""@en;
	rdfs:subClassOf kuno-raeber:Convolute.

kuno-raeber:Diary
	a owl:Class;
	rdfs:label "diary"@en,"Tagebuch"@de;
	rdfs:comment """Diary with entries authored by Kuno Raeber."""@en;
	skos:note """Different from kuno-raeber:DiaryConvolute - in contrast to kuno-raeber:PoemNotebook - because only selections from different diaries are published online."""@en;
	rdfs:subClassOf infocar:Diary, infocar:Manuscript, infocar:Book, infocar:ArchiveElement.

kuno-raeber:DiaryConvolute
	a owl:Class;
	rdfs:label "diary convolute"@en,"Tagebuch-Konvolut"@de;
	rdfs:comment """Convolute of diaries with entries authored by Kuno Raeber."""@en;
	rdfs:subClassOf kuno-raeber:Convolute, infocar:ArchiveElement.

kuno-raeber:Letter
	a owl:Class;
	rdfs:label "letter"@en,"Brief"@de;
	rdfs:comment """Letter written by or to Kuno Raeber."""@en;
	rdfs:subClassOf infocar:Letter, infocar:ArchiveElement, [
		a owl:Restriction; owl:onProperty kuno-raeber:isPartOfLetterConvolute; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:isPartOfLetterConvoluteValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

kuno-raeber:LetterConvolute
	a owl:Class;
	rdfs:label "letter convolute"@en,"Briefekonvolut"@de;
	rdfs:comment """Convolute of letters written by and to Kuno Raeber."""@en;
	rdfs:subClassOf kuno-raeber:Convolute, infocar:ArchiveElement.

kuno-raeber:NachlassPublication
	a owl:Class;
	rdfs:label "nachlass publication"@en, "Nachlass-Publication"@de;
	rdfs:comment """Publication of a Nachlass of Kuno Raeber."""@en;
	rdfs:subClassOf publish:NachlassPublication, infocar:Volume, publish:PrintedPublication.

#
# PROPERTIES
#

kuno-raeber:poemHasTitle
	a owl:ObjectProperty;
	rdfs:label "poem has title"@en, "Gedicht hat Titel"@de;
	rdfs:comment """Relating a poem by Kuno Raeber to a title it has."""@en;
	rdfs:domain kuno-raeber:Poem;
	rdfs:range knora-base:TextValue;
	rdfs:subPropertyOf knora-base:hasValue;
	knora-base:subjectClassConstraint kuno-raeber:Poem;
	knora-base:objectClassConstraint knora-base:TextValue.

kuno-raeber:hasPageIndicator
	a owl:ObjectProperty;
	rdfs:label "has page indicator"@en, "hat Seite-Indikator"@de;
	rdfs:comment """Relating a derived text expression by Kuno Raeber to a diacritical sign that indicates where a page starts."""@en;
	rdfs:domain kuno-raeber:ScientificEdition;
	rdfs:range knora-base:TextValue;
	rdfs:subPropertyOf knora-base:hasValue;
	knora-base:subjectClassConstraint kuno-raeber:ScientificEdition;
	knora-base:objectClassConstraint knora-base:TextValue.

kuno-raeber:hasPageNumberIndicator
	a owl:ObjectProperty;
	rdfs:label "has page number indicator"@en, "hat Seitennummer-Indikator"@de;
	rdfs:comment """Relating a derived text expression by Kuno Raeber to a diacritical sign that indicates the number of a page."""@en;
	rdfs:domain kuno-raeber:ScientificEdition;
	rdfs:range knora-base:TextValue;
	rdfs:subPropertyOf knora-base:hasValue;
	knora-base:subjectClassConstraint kuno-raeber:ScientificEdition;
	knora-base:objectClassConstraint knora-base:TextValue.

kuno-raeber:hasColumnIndicator
	a owl:ObjectProperty;
	rdfs:label "has column indicator"@en, "hat Spalte-Indikator"@de;
	rdfs:comment """Relating a derived text expression by Kuno Raeber to a diacritical sign that indicates where on a page a column starts."""@en;
	rdfs:domain kuno-raeber:ScientificEdition;
	rdfs:range knora-base:TextValue;
	rdfs:subPropertyOf knora-base:hasValue;
	knora-base:subjectClassConstraint kuno-raeber:ScientificEdition;
	knora-base:objectClassConstraint knora-base:TextValue.

kuno-raeber:isOnPage # to be removed when knora-base:hasRepresentation will be implemented in Knora
	a owl:ObjectProperty;
	rdfs:label "is on page"@en, "ist auf Seite"@de;
	rdfs:comment """Relating a handwritten poem by Kuno Raeber to a carrier page it is on."""@en;
